{
  "name": "orderflow",
  "version": "1.0.0",
  "type": "module",
  "description": "OrderFlow é um sistema digital voltado para pontos de alimentação como bares, restaurantes e lanchonetes. O objetivo é facilitar o processo de de pedidos e a organização.",
  "main": "index.js",
  "scripts": {
    "dev": "node --watch server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watchAll --runInBand",
    "services:up": "docker compose -f infra/compose.yaml up -d",
    "services:down": "docker compose -f infra/compose.yaml down"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jest": "^28.11.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "node-pg-migrate": "^7.9.1",
    "prettier": "3.5.3"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "pg": "^8.15.6"
  }
}
